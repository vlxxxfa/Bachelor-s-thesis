@book{Hollosi.2012,
 abstract = {Der heute an Webseiten angelegte Ma{\ss}stab wird zunehmend von Google, Facebook, Twitter  Co bestimmt: Moderne Webseiten zeichnen sich durch sehr hohe Interaktivit{\"a}t und eine internationale Benutzerbasis aus. Diese Komplexit{\"a}t setzt hohe Performanz voraus, um ein intuitives, nahtloses Benutzererlebnis zu gew{\"a}hrleisten. Dieses Buch f{\"u}hrt fortgeschrittene PHP-Programmierer in die Techniken und Methoden moderner Web-Architektur ein. Es bietet L{\"o}sungen f{\"u}r die wichtigsten Aspekte moderner Webseiten: ausgehend von der Vernetzung von Benutzern mit Hilfe von Geodaten {\"u}ber leistungsstarke PHP-Architekturen bis hin zur Verwaltung gro{\ss}er Datenmengen mit NoSQL-L{\"o}sungen.Die Inhalte von Teil I: Vernetzung internationaler Benutzerdaten mit Hilfe von Geodaten // Mehrsprachigkeit // gesteigerte Interaktivit{\"a}t und Responsivit{\"a}t mittels AJAX. Die Inhalte von Teil II: Programmierung von performanten Webseiten // Zwischenspeichern dynamischer Inhalte mittels Caching-L{\"o}sungen // Verwaltung gro{\ss}er Datenmengen mit MySQL bzw. der NoSQL-Datenbank MongoDB.Jedes Kapitel f{\"u}hrt Sie schrittweise in die Thematik ein. Praxisnahe Szenarien und direkt verwendbare Beispiele (Konfigurationen, Quelltexte) stellen einen hohen Bezug zu den typischen Aufgaben des Programmierers her, zahlreiche Praxistipps erleichtern die Umsetzung in eigene Projekte. Insbesondere wird auf typische Fehler hingewiesen, und wie Sie sie vermeiden k{\"o}nnen.Im Internet stehen alle Codes aus dem Buch zur Verf{\"u}gung.{\"U}ber den Autor:Dipl.-Ing. Arno Hollosi ist Dozent und Forscher an der FH CAMPUS 02 im Fachbereich Informationstechnologie. Die Schwerpunkte seiner T{\"a}tigkeit bilden die Themen Webanwendungen und Web-Architekturen.},
 author = {Hollosi, Arno},
 year = {2012},
 title = {Von Geodaten bis NoSQL: leistungsstarke PHP-Anwendungen: Aktuelle Techniken und Methoden f{\"u}r Fortgeschrittene},
 url = {http://www.hanser-elibrary.com/isbn/9783446429956},
